import rsa
(pub, priv) = rsa.newkeys(128)  # 128 bits, mindste antal = 16
print("Hemmeligt primtal p = ", priv.p)
print("Hemmeligt primtal q = ", priv.q)
print("Offentlig nøgle n = ", priv.n)
print("Check: n = p q = ", priv.p * priv.q)
</script></div><div style="margin-top:20px"></div></div><div style="margin-top:20px"></div><div style="margin-top:20px"></div>


